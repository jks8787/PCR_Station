<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @user.name %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<p>
  <strong>Articles:</strong>
  <% @user.articles.each do |article| %>
      <ul>
      <li>title:<%= article.title %> author: <%= article.title %></li>
      </ul>
  <% end %>
</p>

<p>
  <strong>Primers:</strong>
    <% @user.primers.each do |primer| %>
      <ul class="primers">
      <li class="primer">sequence:<%= primer.primer_seq %> note: <%= primer.note %>(<%= link_to 'Show', primer %>| <%= link_to 'Edit', edit_primer_path(primer) %> | <%= link_to 'Destroy', primer, method: :delete, data: { confirm: 'Are you sure?' } %>)
      </li>
      </ul>
  <% end %>

</p>


<%= link_to 'Back', users_path %>

